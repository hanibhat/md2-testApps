package de.wwu.md2.test.models

/*
 * Implement the model here
 */
entity Customer {
	firstName : string(optional)
	lastName : string
	dateOfBirth : date{
		name "Date of birth"
		description "Please specify your date of birth"
	}
}

entity Address {
	street : string(optional)
	zip : string
	city : string
	addressType : AddressType
}

enum AddressType {
	"Private", "Business"
}